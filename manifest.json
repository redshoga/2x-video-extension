{
  "manifest_version": 2,
  "name": "2x Speed",
  "version": "0.0.1",
  "description": "Change video speed on the web page to 2x speed",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  }
}
